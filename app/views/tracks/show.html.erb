<h1 style="font-family: sans-serif" ><%= @track.name %> </h1>
<h2 style="font-family: sans-serif" >- <%= @track.album.name %> (<%= @track.band.name %>) </h2>
<br>

<% @track.notes.each do |note| %>
  <%= note %><br>
<%end%>

<%= link_to "All Albums", albums_url %>
<br>
<%= link_to "All Tracks", tracks_url %>
<br>

<%= link_to "Add an Album", new_band_album_url(@track.band) %>
<br>
<%= link_to "Add a Track", new_album_track_url(@track.album) %>
<br>